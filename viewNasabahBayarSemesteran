        btnKembali.addActionListener(e);
    }

    @Override
    public void viewErrorMsg(String errorMsg) {
        JOptionPane.showMessageDialog(this, errorMsg);
    }
    
    private void data() {
        ResultSet rs = model.getMhsById(model.getSessionIdNasabah());
        try {
            if (rs.next()) {
                setLblNim(rs.getString(1));
                setLblNamaPembayar(rs.getString(2));
                setLblFakultas(rs.getString(4));
                setLblSemester(rs.getString(5));
                setLblStatusBayar(rs.getString(7).equals("B") ? "Belum Lunas" : "Lunas");
                setLblJumlahBayar(rs.getString(6));
                setLblJumlahDenda("0");
                setLblTotalBayar(rs.getString(6));
            }
        } catch (SQLException ex) {
            System.out.println("INFO - Gagal Tampilkan Saldo Nasabah...");
            viewErrorMsg(ex.getMessage());
        }
    }
}
